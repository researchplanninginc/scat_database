<div class="page-header">
  <h1>
    Lookup oil chars
    <a href="/lookup_oil_chars/new" class="btn btn-lg btn-success">New Lookup oil char</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Char code</th>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @lookup_oil_chars.each do |lookup_oil_char| %>
      <tr>
        <td><%= lookup_oil_char.char_code %></td>
        <td><%= lookup_oil_char.description %></td>
        <td>
          <a href="/lookup_oil_chars/<%= lookup_oil_char.id %>" class="btn btn-primary">Show</a>
          <a href="/lookup_oil_chars/<%= lookup_oil_char.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_lookup_oil_char/<%= lookup_oil_char.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

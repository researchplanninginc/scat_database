<div class="page-header">
  <h3>Lookup Esi #<%= @lookup_esi.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Esi code</dt>
      <dd><%= @lookup_esi.esi_code %></dd>

      <dt>Description</dt>
      <dd><%= @lookup_esi.description %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/lookup_esis" class="btn btn-primary">
        Back
      </a>
      <a href="/lookup_esis/<%= @lookup_esi.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_lookup_esi/<%= @lookup_esi.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

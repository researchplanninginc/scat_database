<div class="page-header">
  <h3>Lookup Thick #<%= @lookup_thick.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Thick code</dt>
      <dd><%= @lookup_thick.thick_code %></dd>

      <dt>Description</dt>
      <dd><%= @lookup_thick.description %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/lookup_thicks" class="btn btn-primary">
        Back
      </a>
      <a href="/lookup_thicks/<%= @lookup_thick.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_lookup_thick/<%= @lookup_thick.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

<div class="page-header">
  <h3>Lookup Tidal #<%= @lookup_tidal.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Tidal code</dt>
      <dd><%= @lookup_tidal.tidal_code %></dd>

      <dt>Description</dt>
      <dd><%= @lookup_tidal.description %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/lookup_tidals" class="btn btn-primary">
        Back
      </a>
      <a href="/lookup_tidals/<%= @lookup_tidal.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_lookup_tidal/<%= @lookup_tidal.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
